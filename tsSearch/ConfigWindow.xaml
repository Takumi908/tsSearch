<Window x:Class="tsSearch.ConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tsSearch"
        mc:Ignorable="d"
        Title="ConfigWindow" Height="600" Width="700">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="30*"/>
            <ColumnDefinition Width="30*"/>
            <ColumnDefinition Width="30*"/>
            <ColumnDefinition Width="30*"/>

        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="20*"/>
            <RowDefinition Height="20*"/>
            <RowDefinition Height="20*"/>
            <RowDefinition Height="20*"/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="orignBlock" Text=" " Grid.ColumnSpan="2" Grid.RowSpan="3"/>
        <Button x:Name="btGet" Content="取得" Grid.Column="2"  Margin="8" Width="181" Grid.ColumnSpan="2" Height="31"/>
        <Button x:Name="btEnd" Content="終了" HorizontalAlignment="Left" Margin="47,79,0,0" Grid.Row="3" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <TextBlock x:Name="getBlock" Grid.Column="2"  Margin="10" Grid.Row="2" Text=""  Grid.ColumnSpan="2" Height="264" Grid.RowSpan="2" Width="326"/>

    </Grid>
</Window>
